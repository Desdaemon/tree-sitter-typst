    "numeric": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "float_literal"
            },
            {
              "type": "SYMBOL",
              "name": "int_literal"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "unit"
        }
      ]
    },
    "negative_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
